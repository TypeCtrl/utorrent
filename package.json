{
  "name": "@ctrl/utorrent",
  "version": "0.0.0-placeholder",
  "description": "TypeScript api wrapper for utorrent using got",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "TypeCtrl/utorrent",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "utorrent",
    "typescript"
  ],
  "scripts": {
    "lint": "eslint --ext .js,.ts, .",
    "lint:fix": "eslint --fix --ext .js,.ts, .",
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.build.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --runInBand --reporters=default --coverage",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@ctrl/shared-torrent": "^1.3.2",
    "@ctrl/torrent-file": "^1.0.0",
    "form-data": "^2.5.1",
    "got": "^9.6.0",
    "tough-cookie": "^3.0.1",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@types/got": "9.6.7",
    "@types/jest": "24.0.21",
    "@types/node": "12.12.3",
    "@types/tough-cookie": "2.3.5",
    "@types/url-join": "4.0.0",
    "@typescript-eslint/eslint-plugin": "2.4.0",
    "@typescript-eslint/parser": "2.4.0",
    "eslint": "6.5.1",
    "eslint-config-xo-space": "0.21.0",
    "eslint-config-xo-typescript": "0.19.0",
    "eslint-plugin-import": "2.18.2",
    "jest": "24.9.0",
    "semantic-release": "15.13.24",
    "ts-jest": "24.1.0",
    "typescript": "3.6.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=8"
  }
}
