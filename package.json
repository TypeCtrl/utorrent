{
  "name": "@ctrl/utorrent",
  "version": "0.0.0-placeholder",
  "description": "TypeScript api wrapper for utorrent using got",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "TypeCtrl/utorrent",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "utorrent",
    "typescript"
  ],
  "scripts": {
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts test/**/*.ts",
    "prepare": "npm run build",
    "build": "del dist && tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit --coverage",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@ctrl/shared-torrent": "^1.3.1",
    "@ctrl/torrent-file": "^1.0.0",
    "form-data": "^2.3.3",
    "got": "^9.6.0",
    "tough-cookie": "^3.0.1",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "@types/form-data": "2.2.1",
    "@types/got": "9.4.4",
    "@types/jest": "24.0.13",
    "@types/node": "12.0.5",
    "@types/tough-cookie": "2.3.5",
    "@types/url-join": "4.0.0",
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "@typescript-eslint/parser": "1.9.0",
    "del-cli": "2.0.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-config-xo-space": "0.21.0",
    "eslint-config-xo-typescript": "0.12.0",
    "eslint-plugin-import": "2.17.3",
    "jest": "24.8.0",
    "jest-junit": "6.4.0",
    "semantic-release": "15.13.12",
    "ts-jest": "24.0.2",
    "typescript": "3.5.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=8"
  }
}
