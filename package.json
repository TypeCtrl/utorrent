{
  "name": "@ctrl/utorrent",
  "version": "0.0.0-placeholder",
  "description": "TypeScript api wrapper for utorrent using got",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "scttcper/utorrent",
  "homepage": "https://utorrent.vercel.app",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "utorrent",
    "typescript"
  ],
  "scripts": {
    "lint": "eslint --ext .js,.ts, .",
    "lint:fix": "eslint --fix --ext .js,.ts, .",
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.build.json",
    "build:docs": "typedoc --out docs --hideGenerator --target ES6 --mode file src && touch docs/.nojekyll",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --runInBand --reporters=default --coverage",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@ctrl/shared-torrent": "^3.0.1",
    "@ctrl/torrent-file": "^1.3.0",
    "form-data": "^3.0.0",
    "got": "^11.5.0",
    "tough-cookie": "^4.0.0",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@ctrl/eslint-config": "1.0.3",
    "@jest/globals": "26.1.0",
    "@types/node": "14.0.22",
    "@types/tough-cookie": "4.0.0",
    "@types/url-join": "4.0.0",
    "jest": "26.1.0",
    "ts-jest": "26.1.1",
    "typedoc": "0.17.8",
    "typescript": "3.9.6"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=10.19.0"
  }
}
